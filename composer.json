{
  "name": "tghp/realpage-api",
  "type": "library",
  "description": "A PHP client for the RealPage SOAP API",
  "version": "1.1.0",
  "keywords": [
    "realpage",
    "web services"
  ],
  "authors": [
    {
      "name": "Josh Davenport",
      "email": "josh.davenport@tghp.co.uk",
      "homepage": "https://www.tghp.co.uk"
    }
  ],
  "require": {
    "psr/log": "*",
    "ext-soap": "*",
    "sabre/xml": "2.1.3"
  },
  "require-dev": {
    "doctrine/common": ">=2.3"
  },
  "suggest": {
    "doctrine/common": "For caching SOAP responses",
    "monolog/monolog": "For logging SOAP transactions"
  },
  "autoload": {
    "psr-0": {
      "TGHP\\RealpageClient": "src"
    }
  }
}